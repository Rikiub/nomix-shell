{
    "force_dark_theme": true,
    "matugen": {
        "enabled": true,
        "run_user_config": true,
        "scheme": "tonal-spot"
    },
    "bar": {
		"battery_percent": false
	},
    "clock": {
        "full_date": false,
        "week_day": true,
        "military_time": true,
        "seconds": false
    },
